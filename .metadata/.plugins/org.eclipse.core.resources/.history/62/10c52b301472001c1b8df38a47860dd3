package pages;

import org.openqa.selenium.By;

import webUtills.WebUtills;

public class Report {

	WebUtills utill;
	public Report(WebUtills utill) {
		this.utill=utill;
		// TODO Auto-generated constructor stub
	}
	
	By clkOn_date=By.cssSelector("select#ctl00_ContentPlaceHolder1_ddlEndingDate");
	
	public void focusOnPage(String title) {
		utill.getWindowHandleByTitle(title);
	}
	public void putTheDataOfReport(String date,String companyName) {
		utill.selectByValue(clkOn_date, date);
		By cpmnyName=By.xpath("//td[text()='"+companyName+"']//ancestor::tr//input");
		utill.click_Act(utill.getdriver(), cpmnyName);
		
}
	
	

	
	
	
	
	
	
	
}
