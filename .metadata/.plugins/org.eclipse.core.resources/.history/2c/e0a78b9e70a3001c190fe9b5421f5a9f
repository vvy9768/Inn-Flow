package pages.UserManagment;

import org.openqa.selenium.By;

import webUtills.WebUtills;

public class Users {
	WebUtills utill;
	public Users(WebUtills utill) {
		this.utill=utill;
	}

By firstName=By.cssSelector("input#txtFirstName");
By lastName=By.cssSelector("txtLastName");
	
	
	
	
	
	
	public void validateFirstNameEnability() {
		utill.verifyElement_IsEnable(firstName, "FirstName Field");
		
	}
	
    public void validateLastNameEnability() {
    	utill.verifyElement_IsEnable(lastName, "LastName Field");
	  	
	}
	
	
	
	
	
}
