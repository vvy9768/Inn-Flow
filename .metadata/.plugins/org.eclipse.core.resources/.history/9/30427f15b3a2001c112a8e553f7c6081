package pages;

import org.openqa.selenium.By;

import webUtills.WebUtills;

public class Admin {
	WebUtills utill;

	public Admin(WebUtills utill) {
	   this.utill=utill;	
  
}
	By admin=By.xpath("//img[@alt='Administration']");
	
	public void EnabilityOfUserPage() {
		utill.mouseOver(admin);
		
	}
	
	
	
}